<div class="panel panel-default" id="historico">	
	<div class="panel-heading">			
		<h3>Compras</h3>
	</div>

	<div class="panel-body">	
		<ul>
			<% for(var i = 0; i < historico.length; i++) { %>
				
				<li>
					<a href="venda?id_venda=<%= historico[i].id_venda%>">
						<%=historico[i].nome_produto%> 
						- <small><%=historico[i].data_compra%></small> 
					</a>
				</li>
			<% } %>
		</ul>
	</div>
</div>
